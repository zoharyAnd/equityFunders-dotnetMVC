@model IEnumerable<cFunding.Models.contactus>

@{
    ViewData["Title"] = "Contact us | Create";
}

<!-- contact us -->
<section class="section-contact" id="contact-us">
    <div class="container">
        <h2 class="tcenter">Contact Us</h2>
        <form  asp-action="Edit" enctype="multipart/form-data" id="contact-form">
            <div class="form-group">
                <label for="contact-name" class="control-label">Name</label>
                <input class="form-control" id="contact-name"/>
            </div>
            <div class="form-group">
                <label for="contact-subject" class="control-label">Subject</label>
                <input class="form-control" id="contact-subject"/>
            </div>
            <div class="form-group">
                <label for="contact-message" class="control-label">Message</label>
                <textarea class="form-control" id="contact-message"></textarea>
            </div>
            <div class="contact-btn-wrapper">
                <input type="submit" class="btn" value="Send Message"/>
            </div>
        </form>
    </div>
</section>